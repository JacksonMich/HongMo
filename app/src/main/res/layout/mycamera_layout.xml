<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/p_R1"
        android:layout_width="match_parent"
        android:layout_height="500px"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:id="@+id/zhuangtai"
            android:layout_width="400px"
            android:layout_height="200px"
            android:src="@mipmap/eye"
            android:layout_centerInParent="true"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/zhuangtai"
            android:layout_alignParentTop="true"
            android:id="@+id/relativeLayout4"
            android:layout_marginLeft="75dp"
            android:layout_toLeftOf="@+id/zhuangtai"
            android:layout_toStartOf="@+id/zhuangtai">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView4"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/tuxing2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/zhuangtai"
            android:layout_above="@+id/zhuangtai"
            android:layout_toRightOf="@+id/zhuangtai"
            android:id="@+id/relativeLayout2"
            android:layout_marginRight="75dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView5"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/tuxing3" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/zhuangtai"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/zhuangtai"
            android:id="@+id/relativeLayout"
            android:layout_toLeftOf="@+id/zhuangtai"
            android:layout_marginLeft="75dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView6"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/tuxing1" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignTop="@+id/relativeLayout"
            android:layout_toEndOf="@+id/relativeLayout"
            android:layout_alignLeft="@+id/relativeLayout2"
            android:layout_alignStart="@+id/relativeLayout2"
            android:id="@+id/relativeLayout3"
            android:layout_marginRight="75dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView7"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/tuxing4" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="440px"
            android:layout_height="430px"
            android:layout_above="@+id/zhuangtai"
            android:layout_toLeftOf="@+id/relativeLayout2"
            android:layout_toRightOf="@+id/relativeLayout4"
            android:layout_toEndOf="@+id/relativeLayout4">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView8"
                android:src="@mipmap/jianbianxian"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="70px" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignTop="@+id/zhuangtai"
            android:layout_toStartOf="@+id/zhuangtai"
            android:layout_toLeftOf="@+id/zhuangtai"
            android:layout_above="@+id/relativeLayout"
            android:layout_marginLeft="75dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView10"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/jianbianxian1" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignLeft="@+id/relativeLayout2"
            android:layout_alignStart="@+id/relativeLayout2"
            android:layout_below="@+id/relativeLayout2"
            android:layout_above="@+id/relativeLayout3"
            android:layout_marginRight="75dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView11"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/jianbianxian1" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/zhuangtai"
            android:layout_toRightOf="@+id/relativeLayout"
            android:layout_toLeftOf="@+id/relativeLayout3"
            android:layout_toStartOf="@+id/relativeLayout3">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView9"
                android:src="@mipmap/jianbianxian"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="70px" />
        </RelativeLayout>

    </RelativeLayout>

    <Button
        android:id="@+id/quitAccount"
        android:layout_width="250px"
        android:layout_height="79px"
        android:layout_marginTop="220px"
        android:background="@mipmap/anniu_queren"
        android:textSize="@dimen/font_size_12"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/p_R1" />


    <include
        layout="@layout/advertise"
     />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="吴德江"
        android:id="@+id/textView"
        android:layout_below="@+id/p_R1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30px"
        android:textColor="#ff3000" />
</RelativeLayout>