<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/p_R1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:background="@mipmap/img1">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#000000"
            android:layout_alignParentTop="true"
            android:alpha="0.5">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="243dp"
            android:layout_height="81.7dp"
            android:layout_alignTop="@+id/relativeLayout"
            android:layout_alignLeft="@+id/relativeLayout"
            android:layout_alignStart="@+id/relativeLayout"
            android:layout_alignBottom="@+id/relativeLayout"
            android:background="@mipmap/eye"
            android:layout_alignRight="@+id/relativeLayout"
            android:layout_alignEnd="@+id/relativeLayout"></RelativeLayout>

        <RelativeLayout
            android:layout_width="243dp"
            android:layout_height="101.7dp"
            android:background="@mipmap/img2"
            android:layout_marginTop="146dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="85dp"
            android:id="@+id/relativeLayout"
            android:alpha="0.5">

        </RelativeLayout>

    </RelativeLayout>


    <include
        layout="@layout/advertise"
     />

</RelativeLayout>